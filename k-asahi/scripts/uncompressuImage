cd /boot
objcopy -O binary vmlinuz-kirkwood vmlinuz-kirkwood.bin

mkimage \
   -A arm \
   -O linux \
   -T kernel \
   -n "Slackware ARM Linux Kernel" \
   -d vmlinuz-kirkwood.bin \
   uImage-kirkwood_uncompressed \
   


